{
    "name": "Dev ext",
    "version": "1.0",
    "description": "It changes images of webpage",
    "manifest_version": 2,
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "css": ["myStyles.css"],
        "js": ["contentScript.js"]
    }],
    "browser_action": {
        "default_popup": "hello.html"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    }
}